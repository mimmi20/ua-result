{
  "name": "mimmi20/ua-result",
  "type": "library",
  "description": "Library for PHP 7.1+ to handle useragent parser results",
  "keywords": [
    "Browser",
    "user agent",
    "http",
    "parser",
    "user-agent"
  ],
  "homepage": "https://github.com/mimmi20/ua-result",
  "license": "MIT",
  "authors": [
    {
      "name": "Thomas MÃ¼ller",
      "homepage": "https://github.com/mimmi20",
      "role": "Developer"
    },
    {
      "name": "Contributors",
      "homepage": "https://github.com/mimmi20/ua-result/graphs/contributors"
    }
  ],
  "require": {
    "php": "^7.1",
    "ext-json": "*",
    "mimmi20/browser-detector-version": "~6.0.0",
    "mimmi20/ua-browser-type": "~7.0.0",
    "mimmi20/ua-device-type": "~7.0.0",
    "mimmi20/ua-display-size": "~1.0.0",
    "psr/log": "^1.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.13",
    "infection/infection": "^0.11.2",
    "localheinz/composer-normalize": "^1.0.0",
    "localheinz/phpstan-rules": "^0.4",
    "mimmi20/json-class": "~1.0.0",
    "pepakriz/phpstan-exception-rules": "^0.3",
    "phpstan/phpstan": "^0.10",
    "phpstan/phpstan-beberlei-assert": "^0.10",
    "phpstan/phpstan-deprecation-rules": "^0.10",
    "phpstan/phpstan-phpunit": "^0.10",
    "phpstan/phpstan-strict-rules": "^0.10",
    "phpunit/phpunit": "^7.4",
    "squizlabs/php_codesniffer": "^3.3"
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "UaResult\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "UaResultTest\\": "tests/"
    }
  },
  "support": {
    "issues": "https://github.com/mimmi20/ua-result/issues",
    "source": "https://github.com/mimmi20/ua-result"
  }
}
