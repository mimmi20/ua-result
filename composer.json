{
  "name": "mimmi20/ua-result",
  "description": "Library for PHP 7.4+ to handle useragent parser results",
  "license": "MIT",
  "type": "library",
  "keywords": [
    "Browser",
    "user agent",
    "http",
    "parser",
    "user-agent"
  ],
  "authors": [
    {
      "name": "Thomas MÃ¼ller",
      "homepage": "https://github.com/mimmi20",
      "role": "Developer"
    },
    {
      "name": "Contributors",
      "homepage": "https://github.com/mimmi20/ua-result/graphs/contributors"
    }
  ],
  "homepage": "https://github.com/mimmi20/ua-result",
  "support": {
    "issues": "https://github.com/mimmi20/ua-result/issues",
    "source": "https://github.com/mimmi20/ua-result"
  },
  "require": {
    "php": "^8.1.0",
    "ext-json": "*",
    "ext-mbstring": "*",
    "mimmi20/browser-detector-version": "^7.0.8",
    "mimmi20/ua-browser-type": "^8.0.6",
    "mimmi20/ua-device-type": "^8.0.6",
    "psr/log": "^1.1.4 || ^2.0.0 || ^3.0.0"
  },
  "require-dev": {
    "ext-ctype": "*",
    "ext-dom": "*",
    "ext-simplexml": "*",
    "ext-tokenizer": "*",
    "ext-xml": "*",
    "ext-xmlwriter": "*",
    "infection/infection": "^0.27.0",
    "mimmi20/coding-standard": "^5.1.7",
    "nikic/php-parser": "^4.16.0",
    "phpstan/extension-installer": "^1.3.1",
    "phpstan/phpstan": "^1.10.23",
    "phpstan/phpstan-deprecation-rules": "^1.1.3",
    "phpstan/phpstan-phpunit": "^1.3.13",
    "phpunit/phpunit": "^10.2.3",
    "rector/rector": "^0.17.2",
    "vimeo/psalm": "^5.13.1"
  },
  "autoload": {
    "psr-4": {
      "UaResult\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "UaResultTest\\": "tests/"
    }
  },
  "config": {
    "preferred-install": "stable",
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "phpstan/extension-installer": true,
      "infection/extension-installer": true
    }
  }
}
