{
  "name": "mimmi20/ua-result",
  "type": "library",
  "description": "Library for PHP 7.1+ to handle useragent parser results",
  "keywords": [
    "Browser",
    "user agent",
    "http",
    "parser",
    "user-agent"
  ],
  "homepage": "https://github.com/mimmi20/ua-result",
  "license": "MIT",
  "authors": [
    {
      "name": "Thomas MÃ¼ller",
      "homepage": "https://github.com/mimmi20",
      "role": "Developer"
    },
    {
      "name": "Contributors",
      "homepage": "https://github.com/mimmi20/ua-result/graphs/contributors"
    }
  ],
  "require": {
    "php": "^7.1.3 || ^8.0.0",
    "ext-json": "*",
    "mimmi20/browser-detector-version": "~6.0.16",
    "mimmi20/ua-browser-type": "~7.0.14",
    "mimmi20/ua-device-type": "~7.1.13",
    "mimmi20/ua-display-size": "~2.0.24",
    "psr/log": "~1.1.3"
  },
  "require-dev": {
    "infection/infection": "~0.13.6 || ~0.18",
    "mimmi20/coding-standard": "~1.0.6",
    "mimmi20/json-class": "~2.0.15",
    "pepakriz/phpstan-exception-rules": "~0.11.2",
    "phpstan/extension-installer": "~1.0.5",
    "phpstan/phpstan": "~0.12.57",
    "phpstan/phpstan-deprecation-rules": "~0.12.5",
    "phpstan/phpstan-phpunit": "~0.12.16",
    "phpunit/phpunit": "~7.5.20 || ~8.5.8 || ~9.4.2"
  },
  "config": {
    "platform": {
      "php": "7.1.3"
    },
    "preferred-install": "stable",
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "UaResult\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "UaResultTest\\": "tests/"
    }
  },
  "support": {
    "issues": "https://github.com/mimmi20/ua-result/issues",
    "source": "https://github.com/mimmi20/ua-result"
  }
}
