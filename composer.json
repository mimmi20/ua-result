{
  "name": "mimmi20/ua-result",
  "type": "library",
  "description": "Library for PHP 7.1+ to handle useragent parser results",
  "keywords": [
    "Browser",
    "user agent",
    "http",
    "parser",
    "user-agent"
  ],
  "homepage": "https://github.com/mimmi20/ua-result",
  "license": "MIT",
  "authors": [
    {
      "name": "Thomas MÃ¼ller",
      "homepage": "https://github.com/mimmi20",
      "role": "Developer"
    },
    {
      "name": "Contributors",
      "homepage": "https://github.com/mimmi20/ua-result/graphs/contributors"
    }
  ],
  "require": {
    "php": "^7.1",
    "ext-json": "*",
    "mimmi20/browser-detector-version": "~6.0.3",
    "mimmi20/ua-browser-type": "~7.0.3",
    "mimmi20/ua-device-type": "~7.1.3",
    "mimmi20/ua-display-size": "~1.0.17",
    "psr/log": "^1.0.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.16.0",
    "infection/infection": "^0.13.6",
    "localheinz/composer-normalize": "^1.3.1",
    "localheinz/phpstan-rules": "^0.13.0",
    "mimmi20/json-class": "~2.0.3",
    "pepakriz/phpstan-exception-rules": "^0.9.0",
    "phpstan/phpstan": "^0.11.19",
    "phpstan/phpstan-deprecation-rules": "^0.11.2",
    "phpstan/phpstan-phpunit": "^0.11.2",
    "phpstan/phpstan-strict-rules": "^0.11.1",
    "phpunit/phpunit": "^7.5.17 || ^8.4.0",
    "squizlabs/php_codesniffer": "^3.5.2"
  },
  "config": {
    "platform": {
      "php": "7.1.3"
    },
    "preferred-install": "stable",
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "UaResult\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "UaResultTest\\": "tests/"
    }
  },
  "support": {
    "issues": "https://github.com/mimmi20/ua-result/issues",
    "source": "https://github.com/mimmi20/ua-result"
  }
}
