{
  "name": "mimmi20/ua-result",
  "type": "library",
  "description": "Library for PHP 7.4+ to handle useragent parser results",
  "keywords": [
    "Browser",
    "user agent",
    "http",
    "parser",
    "user-agent"
  ],
  "homepage": "https://github.com/mimmi20/ua-result",
  "license": "MIT",
  "authors": [
    {
      "name": "Thomas MÃ¼ller",
      "homepage": "https://github.com/mimmi20",
      "role": "Developer"
    },
    {
      "name": "Contributors",
      "homepage": "https://github.com/mimmi20/ua-result/graphs/contributors"
    }
  ],
  "require": {
    "php": "^7.4.0 || ^8.0.0",
    "ext-json": "*",
    "mimmi20/browser-detector-version": "^7.0.0",
    "mimmi20/ua-browser-type": "^8.0.0",
    "mimmi20/ua-device-type": "^8.0.0",
    "psr/log": "^1.1.3"
  },
  "require-dev": {
    "infection/infection": "^0.21.4",
    "mimmi20/coding-standard": "^2.0.1",
    "mimmi20/json-class": "^3.0.0",
    "pepakriz/phpstan-exception-rules": "^0.11.6",
    "phpstan/extension-installer": "^1.1.0",
    "phpstan/phpstan": "^0.12.80",
    "phpstan/phpstan-deprecation-rules": "^0.12.6",
    "phpstan/phpstan-phpunit": "^0.12.18",
    "phpunit/phpunit": "^9.5.2"
  },
  "config": {
    "preferred-install": "stable",
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "UaResult\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "UaResultTest\\": "tests/"
    }
  },
  "support": {
    "issues": "https://github.com/mimmi20/ua-result/issues",
    "source": "https://github.com/mimmi20/ua-result"
  }
}
