{
  "name": "mimmi20/ua-result",
  "type": "library",
  "description": "Library for PHP 7.1+ to handle useragent parser results",
  "keywords": [
    "Browser",
    "user agent",
    "http",
    "parser",
    "user-agent"
  ],
  "homepage": "https://github.com/mimmi20/ua-result",
  "license": "MIT",
  "authors": [
    {
      "name": "Thomas MÃ¼ller",
      "homepage": "https://github.com/mimmi20",
      "role": "Developer"
    },
    {
      "name": "Contributors",
      "homepage": "https://github.com/mimmi20/ua-result/graphs/contributors"
    }
  ],
  "require": {
    "php": "^7.1.3 || ^8.0.0",
    "ext-json": "*",
    "mimmi20/browser-detector-version": "~6.0.12",
    "mimmi20/ua-browser-type": "~7.0.10",
    "mimmi20/ua-device-type": "~7.1.9",
    "mimmi20/ua-display-size": "~2.0.14",
    "psr/log": "^1.1.3"
  },
  "require-dev": {
    "ergebnis/composer-normalize": "^2.6.1",
    "ergebnis/phpstan-rules": "^0.15.1",
    "infection/infection": "^0.13.6 || ^0.16.3",
    "mimmi20/coding-standard": "~1.0.1",
    "mimmi20/json-class": "~2.0.11",
    "pepakriz/phpstan-exception-rules": "^0.11.1",
    "phpstan/extension-installer": "^1.0.4",
    "phpstan/phpstan": "^0.12.34",
    "phpstan/phpstan-deprecation-rules": "^0.12.5",
    "phpstan/phpstan-phpunit": "^0.12.15",
    "phpstan/phpstan-strict-rules": "^0.12.4",
    "phpunit/phpunit": "^7.5.20 || ^8.5.5 || ^9.1.5"
  },
  "config": {
    "platform": {
      "php": "7.1.3"
    },
    "preferred-install": "stable",
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "UaResult\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "UaResultTest\\": "tests/"
    }
  },
  "support": {
    "issues": "https://github.com/mimmi20/ua-result/issues",
    "source": "https://github.com/mimmi20/ua-result"
  }
}
