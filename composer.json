{
  "name": "mimmi20/ua-result",
  "description": "Library for PHP 7.4+ to handle useragent parser results",
  "license": "MIT",
  "type": "library",
  "keywords": [
    "Browser",
    "user agent",
    "http",
    "parser",
    "user-agent"
  ],
  "authors": [
    {
      "name": "Thomas MÃ¼ller",
      "homepage": "https://github.com/mimmi20",
      "role": "Developer"
    },
    {
      "name": "Contributors",
      "homepage": "https://github.com/mimmi20/ua-result/graphs/contributors"
    }
  ],
  "homepage": "https://github.com/mimmi20/ua-result",
  "support": {
    "issues": "https://github.com/mimmi20/ua-result/issues",
    "source": "https://github.com/mimmi20/ua-result"
  },
  "require": {
    "php": "^8.1.0",
    "ext-mbstring": "*",
    "mimmi20/browser-detector-version": "^8.0.1",
    "mimmi20/ua-browser-type": "^9.0.1",
    "mimmi20/ua-device-type": "^9.0.1"
  },
  "require-dev": {
    "ext-ctype": "*",
    "ext-dom": "*",
    "ext-simplexml": "*",
    "ext-tokenizer": "*",
    "ext-xml": "*",
    "ext-xmlwriter": "*",
    "infection/infection": "^0.27.0",
    "mimmi20/coding-standard": "^5.1.13",
    "nikic/php-parser": "^4.17.1",
    "phpstan/extension-installer": "^1.4.3",
    "phpstan/phpstan": "^1.10.33",
    "phpstan/phpstan-deprecation-rules": "^1.1.4",
    "phpstan/phpstan-phpunit": "^1.3.14",
    "phpunit/phpunit": "^10.3.2",
    "rector/rector": "^0.18.1",
    "vimeo/psalm": "^5.26.1"
  },
  "autoload": {
    "psr-4": {
      "UaResult\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "UaResultTest\\": "tests/"
    }
  },
  "config": {
    "preferred-install": "dist",
    "lock": false,
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "phpstan/extension-installer": true,
      "infection/extension-installer": true
    }
  }
}
