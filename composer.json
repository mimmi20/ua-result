{
  "name": "mimmi20/ua-result",
  "type": "library",
  "description": "Library for PHP 7.1+ to handle useragent parser results",
  "keywords": [
    "Browser",
    "user agent",
    "http",
    "parser",
    "user-agent"
  ],
  "homepage": "https://github.com/mimmi20/ua-result",
  "license": "MIT",
  "authors": [
    {
      "name": "Thomas MÃ¼ller",
      "homepage": "https://github.com/mimmi20",
      "role": "Developer"
    },
    {
      "name": "Contributors",
      "homepage": "https://github.com/mimmi20/ua-result/graphs/contributors"
    }
  ],
  "require": {
    "php": "^7.1.3 || ^8.0.0",
    "ext-json": "*",
    "mimmi20/browser-detector-version": "~6.0.11",
    "mimmi20/ua-browser-type": "~7.0.9",
    "mimmi20/ua-device-type": "~7.1.8",
    "mimmi20/ua-display-size": "~2.0.7",
    "psr/log": "^1.1.3"
  },
  "require-dev": {
    "ergebnis/composer-normalize": "^2.5.1",
    "ergebnis/phpstan-rules": "^0.15.0",
    "friendsofphp/php-cs-fixer": "^2.16.3",
    "infection/infection": "^0.13.6 || ^0.16.3",
    "mimmi20/json-class": "~2.0.10",
    "pepakriz/phpstan-exception-rules": "^0.10.1",
    "phpstan/phpstan": "^0.12.25",
    "phpstan/phpstan-deprecation-rules": "^0.12.2",
    "phpstan/phpstan-phpunit": "^0.12.8",
    "phpstan/phpstan-strict-rules": "^0.12.2",
    "phpunit/phpunit": "^7.5.20 || ^8.5.4 || ^9.1.3",
    "squizlabs/php_codesniffer": "^3.5.5"
  },
  "config": {
    "platform": {
      "php": "7.1.3"
    },
    "preferred-install": "stable",
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "UaResult\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "UaResultTest\\": "tests/"
    }
  },
  "support": {
    "issues": "https://github.com/mimmi20/ua-result/issues",
    "source": "https://github.com/mimmi20/ua-result"
  }
}
