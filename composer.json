{
  "name": "mimmi20/ua-result",
  "type": "library",
  "description": "Library for PHP 7.1+ to handle useragent parser results",
  "keywords": [
    "Browser",
    "user agent",
    "http",
    "parser",
    "user-agent"
  ],
  "homepage": "https://github.com/mimmi20/ua-result",
  "license": "MIT",
  "authors": [
    {
      "name": "Thomas MÃ¼ller",
      "homepage": "https://github.com/mimmi20",
      "role": "Developer"
    },
    {
      "name": "Contributors",
      "homepage": "https://github.com/mimmi20/ua-result/graphs/contributors"
    }
  ],
  "require": {
    "php": "^7.1",
    "ext-json": "*",
    "mimmi20/browser-detector-version": "~6.0.3",
    "mimmi20/ua-browser-type": "~7.0.2",
    "mimmi20/ua-device-type": "~7.1.0",
    "mimmi20/ua-display-size": "~1.0.15",
    "psr/log": "^1.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.14",
    "infection/infection": "^0.12.0",
    "localheinz/composer-normalize": "^1.1.0",
    "localheinz/phpstan-rules": "^0.7 || ^0.8 || ^0.9",
    "mimmi20/json-class": "~2.0.2",
    "pepakriz/phpstan-exception-rules": "^0.6 || ^0.8",
    "phpstan/phpstan": "^0.11",
    "phpstan/phpstan-beberlei-assert": "^0.11",
    "phpstan/phpstan-deprecation-rules": "^0.11",
    "phpstan/phpstan-phpunit": "^0.11",
    "phpstan/phpstan-strict-rules": "^0.11",
    "phpunit/phpunit": "^7.5 || ^8.0",
    "squizlabs/php_codesniffer": "^3.4"
  },
  "config": {
    "preferred-install": "stable",
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "UaResult\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "UaResultTest\\": "tests/"
    }
  },
  "support": {
    "issues": "https://github.com/mimmi20/ua-result/issues",
    "source": "https://github.com/mimmi20/ua-result"
  }
}
